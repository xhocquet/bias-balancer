{
	"name": "BiasBalancer",
	"version": "0.0.1",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"activeTab"
	],
	"background": {
		"scripts": [
			"browser-polyfill.min.js",
			"background.js"
		]
	},
	"content_scripts": [{
    "matches": [ "*://*/*" ],
    "js": ["content.js"]
   }]
}
